    <eStructuralFeatures xsi:type="ecore:EReference" name="msTerm" lowerBound="1"
        eType="#//MSTerm"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="MSTerm">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="placeId" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="multiplicity" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="value" lowerBound="1" eType="ecore:EClass runtime.ecore#//AbstractValue"/>
  </eClassifiers>
</ecore:EPackage>
